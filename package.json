{
  "name": "vue-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-localhost": "vue-cli-service serve --host localhost",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
